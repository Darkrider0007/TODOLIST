
    <%- include("header") -%>
    <div class="app">
        <h1 class="date" id="name">TO DO LIST</h1>
    </div>


    <div class="box" id="heading">
        <h1 class="date">
            <%= ListTitle %>
        </h1>
    </div>

    <!-- <li>Blender</li>
        <li>Python csb-excel</li>
        <li>DSA</li> -->
    <div class="box">
        <% for(let i=0; i<newListItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= newListItems[i] %>
                </p>
            </div>
            <% } %>
                <form class="item" action="/" method="post">
                    <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
                    <button type="submit" name="list" value=<%= ListTitle %>>+</button>
                </form>
    </div>

    <%- include("footer") -%>


